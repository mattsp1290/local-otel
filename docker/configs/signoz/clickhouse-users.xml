<clickhouse>
    <profiles>
        <default>
            <max_execution_time>300</max_execution_time>
            <max_memory_usage>10000000000</max_memory_usage>
            <max_memory_usage_for_all_queries>10000000000</max_memory_usage_for_all_queries>
            <max_query_size>1048576</max_query_size>
            <max_ast_elements>1000000</max_ast_elements>
            <max_expanded_ast_elements>1000000</max_expanded_ast_elements>
            <log_queries>1</log_queries>
            <log_query_threads>1</log_query_threads>
        </default>
    </profiles>

    <users>
        <signoz>
            <password>signoz</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </signoz>

        <default>
            <password></password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>
    </users>

    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</clickhouse>
